<%=  popup_title "Input Tipe Kas Bank"  %>

<div id="RB_body" style="width: 350px;">
  <% form_for(:accounting_bank_cash, @accounting_bank_cash, :url => {:action => 'create'})  do |f| %>
    <%= f.error_messages %>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <p>
      <%= f.submit "Simpan" %>
    </p>
  <% end %>
</div>