<div class="div_top_btn">
  <%= link_to("Kembali",nota_orders_path,{:class => "a_top_btn"}) %>
</div>
<div class="clear_right"></div>
<table class="main_table" cellpadding="0" cellspacing="0" width="400" style="float:left;">
  <tr class="grid_1">
    <td style="width :100px; text-align :center;">No Rekening</td>
    <td style="width :200px; text-align :center;">Keterangan</td>
    <td>&nbsp;</td>
    <td>&nbsp;</td>
  </tr>
  <% @cogs_accounts.each do |cogs_account| %>
    <%= display_single_debit_credit_type cogs_account[:label] , cogs_account[:account], cogs_account[:account_type] %>
  <% end %>
</table>

