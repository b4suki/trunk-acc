<%=  popup_title "Edit Pembayaran Piutang" %>

<div id="RB_body" style="width: 500px;">
<% form_for(:receivable, @receivable, :url => accounting_receivable_maturity_path(@receivable), :html=> {:method=>'put'})  do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <div style="clear: both;"></div>
  <%= f.submit "Update" %>
<% end %>
</div>