<%=  popup_title "Edit Transaksi Kas" %>

<div id="RB_body" style="width: 500px;">
<% form_for(:cash_balance, @cash_balance, :url => accounting_cash_balance_path(@cash_balance), :html=> {:method=>'put'})  do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <div style="clear: both;"></div>
  <%= f.submit "Update" %>
<% end %>
</div>