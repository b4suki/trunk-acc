<%=  popup_title "Edit Pajak" %>

<div id="RB_body" style="width: 350px;">
<% form_for(:tax, @tax, :url => accounting_taxis_path(@tax), :html=> {:method=>'put'})  do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>
</div>