<%=  popup_title "Input Penanda tangan" %>

<div id="RB_body" style="width: 300px;">
<% form_for(:sale_signature, @sale_signature, :url => {:action => 'create'})  do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <p>
    <%= f.submit "Simpan" %>
  </p>
<% end %>
</div>