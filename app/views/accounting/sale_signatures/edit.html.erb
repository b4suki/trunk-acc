<%=  popup_title "Edit Penanda tangan" %>

<div id="RB_body" style="width: 300px;">
<% form_for(:sale_signature, @sale_signature, :url => accounting_sale_signature_path(@sale_signature), :html=> {:method=>'put'})  do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>
</div>