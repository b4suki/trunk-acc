<%=  popup_title "Edit Pembayaran Utang" %>

<div id="RB_body" style="width: 500px;">
<% form_for(:liability, @liability, :url => accounting_liability_maturity_path(@liability), :html=> {:method=>'put'})  do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <div style="clear: both;"></div>
  <%= f.submit "Update" %>
<% end %>
</div>