<h1>New reconciliation detail</h1>

<% form_for(:reconciliation_detail, @reconciliation_detail, :url => {:action => 'create'}) do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', accounting_reconciliation_details_path %>
