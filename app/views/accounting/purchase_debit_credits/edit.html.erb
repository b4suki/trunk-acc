<%=  popup_title "Edit Tipe Debit Credit Pembelian" %>

<div id="RB_body" style="width: 350px;">
<% form_for(:purchase_debit_credit, @purchase_debit_credit, :url => accounting_purchase_debit_credit_path(@purchase_debit_credit), :html=> {:method=>'put'})  do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <p>
    <%= f.submit "Update" %>  
  </p>
<% end %>
</div>