<%= popup_title "Edit Manual Journal" %>
<div style="max-height:400px; overflow-x:hidden;overflow-y:auto;">
<div id="RB_body" style="width: 850px;">
<% form_for(:manual_journal, @manual_journal, :url => {:action => 'update'}, :html => {:method => :put})  do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form_edit', :locals => { :f => f } %>
  <br />
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>
</div>
</div>