<%=  popup_title "Input Tipe Debit Credit Penjualan" %>

<div id="RB_body" style="width: 350px;">
<% form_for(:sale_debit_credit, @sale_debit_credit, :url => {:action => 'create'})  do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <p>
    <%= f.submit "Simpan" %>  
  </p>
<% end %>
</div>