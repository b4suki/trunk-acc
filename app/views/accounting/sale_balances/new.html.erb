<%=  popup_title "Input Transaksi Penjualan" %>
<div style="max-height:410px; overflow:auto;">
  <div id="RB_body" style="width: 700px;">
    <% form_for(:sale_balance, @sale_balance, :url => {:action => 'create'})  do |f| %>
      <%= f.error_messages %>
      <%= render :partial => 'form', :locals => { :f => f } %>
      <p>
        <%= f.submit "Simpan" %>
      </p>
    <% end %>
  </div>
</div>