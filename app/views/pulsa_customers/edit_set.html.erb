<%= popup_title "Setting Item Pulsa " %>

<div id="RB_body" style="width: 300px;">
  <%= javascript_include_tag :defaults %>
  <% form_tag :action => 'edit_setting_pulsa' do %>
    <p>
      Setting <br/>
      <%= text_field_with_auto_complete :item, :name, {:value => @item.name}, :skip_style => true, :indicator => "indicator-item-name" -%>
      <%= hidden_field_tag :item_id,@item.id,:id => 'item_name_id' %>
      <%= hidden_field_tag :set_id, @setting.id %>
    </p>

    <p>
      <%= submit_tag "Create" %>
    </p>
  <% end %>
</div>
